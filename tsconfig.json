{
  "compilerOptions": {
    "resolveJsonModule": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6"
    ],
    "types": [],
    "downlevelIteration": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "target": "es5",
    "esModuleInterop": true
  },
  "include": [
    "src/**/*",
    "postcss.config.js"
  ]
}
